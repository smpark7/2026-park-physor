	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/00295450.2024.2409601},
	keywords = {SAM, safety analysis, advanced reactors, systems code},
	pages = {1883--1902},
	file = {Full Text PDF:C\:\\Users\\sp58763\\Zotero\\storage\\RSIG84B8\\Hu et al. - 2025 - SAM A Modern System Code for Advanced Non-LWR Safety Analysis.pdf:application/pdf},
